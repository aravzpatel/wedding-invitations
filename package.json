    "react-test-renderer": "^16.13.1",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/app/javascript/tests"
    ],
    "verbose": true,
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/app/javascript"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "\\.svg": "<rootDir>/app/javascript/tests/__mocks__/svgMock.js"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./app/javascript/tests/setupTests.js"
    ]
  },
  "scripts": {
    "test": "jest"
  }
